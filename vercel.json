{
    "headers": [
      {
        "source": "/IMAGENES/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]      
      }
    ], 

    "redirects": [
        { "source": "/index.html", "destination": "/", "permanent": true }
    ],

    "rewrites": [
      { "source": "/balnearios", "destination": "/balnearios.html" },
      { "source": "/complejo", "destination": "/complejo.html" },
      { "source": "/ubicacion", "destination": "/ubicacion.html" }
    ]
    
  }
  